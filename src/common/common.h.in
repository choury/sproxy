#define  LOG(...)  slog(LOG_INFO,  __VA_ARGS__)
#define  LOGE(...) \
do{\
    char tmp[1024]; \
    int __ret = snprintf(tmp, sizeof(tmp), __VA_ARGS__); \
    if(__ret < 0) abort(); \
    slog(LOG_ERR, "%s[%d]: %s", __PRETTY_FUNCTION__, __LINE__, tmp);\
}while(0)

#define  LOGF(...) \
do{\
    LOGE(__VA_ARGS__); \
    abort(); \
}while(0)

forceinline uint64_t getutime(){
    struct timeval tv;
    gettimeofday(&tv, NULL);
    return tv.tv_sec * 1000000ull + tv.tv_usec;
}

forceinline uint32_t getmtime(){
    struct timeval tv;
    gettimeofday(&tv, 0);
    return (tv.tv_sec * 1000ull + tv.tv_usec/1000)&0xFFFFFFFF;
}

#define  LOGD(mod, ...)  \
do{\
    if(debug & mod) {\
        char tmp[1024]; \
        int __ret =snprintf(tmp, sizeof(tmp), __VA_ARGS__); \
        if(__ret < 0) abort(); \
        slog(LOG_DEBUG, "%06u:%s: %s", getmtime()%1000000u, mod##_STR, tmp); \
    }\
}while(0)


